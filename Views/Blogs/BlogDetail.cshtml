
@{
    ViewBag.Title = "BlogDetail";
}
<script lang="ja" type="text/javascript" src="~/Scripts/BlogDetail.js"></script>
<div class="container">
    <div class="row">
        <div class="col-md-6col-md-offset-3">
            <h2>Blog Detail</h2>
            <div class="row">
                <div class="col-md-12">
                    <div class="table-responsive">
                        <table class="table" id="tblBlogDetail">
                            <tr>
                                <th class="col-md-3">&nbsp; @Html.Hidden("BlogId", (object)ViewBag.BlogId)</th>
                                <td class="col-md-9">&nbsp;</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group col-lg-6">
                        <a class="btn btn-danger form-control" id="btnBack" href="/"><i class="glyphiconglyphicon-log-in"></i><< Back</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
